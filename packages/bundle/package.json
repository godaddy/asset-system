{
  "name": "asset-bundle",
  "version": "4.1.0",
  "description": "Build process that transforms (multiple) SVG assets into a asset-parser compatible bundle",
  "main": "./lib",
  "browser": "./lib",
  "module": "./index",
  "react-native": "./index",
  "scripts": {
    "test:web": "mocha --colors --require setup-env test/*.test.js",
    "test": "npm run build && nyc --reporter=text --reporter=lcov npm run test:web",
    "prepublishOnly": "npm run build",
    "build": "rm -rf ./lib && babel ./index.js -d ./lib && babel ./walker/*.js -d ./lib/walker",
    "lint": "eslint-godaddy *.js ./test/*.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "asset-dimensions": "^1.0.0",
    "asset-parser": "^3.0.0",
    "async": "^2.6.0",
    "camel-case": "^3.0.0",
    "cheerio": "^1.0.0-rc.2",
    "diagnostics": "^1.1.0",
    "react": "^16.2.0",
    "svgo": "^1.0.3",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "asset-test": "^1.0.0",
    "assume": "^1.5.2",
    "eslint": "^4.14.0",
    "eslint-config-godaddy": "^2.1.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "puppeteer": "^1.1.0",
    "setup-env": "^1.2.1"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 9
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "nyc": {
    "exclude": [
      "lib"
    ]
  },
  "homepage": "https://github.com/godaddy/asset-system/tree/master/packages/bundle",
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/asset-system.git"
  },
  "keywords": [
    "SVG",
    "asset",
    "bundle"
  ],
  "author": "GoDaddy.com Operating Company, LLC",
  "contributors": [
    "Arnout Kazemier <akazemier@godaddy.com>"
  ],
  "license": "MIT",
  "snyk": true
}
